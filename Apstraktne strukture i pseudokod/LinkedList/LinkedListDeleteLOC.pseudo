// Napisati pseudokod operacije brisanja elementa čija je lokacija LOC
// iz jednostruko spregnute lančane liste LL.

Delete(START, PREV, LOC)
    if (START = LOC) // ili PREV = NULL
        START <- link(LOC) // ili <- link(START)
    else
        link(PREV) <- link(LOC)
    free(LOC)
    return

// O(1) !!